{
  "name": "@findy-network/findy-common-ts",
  "version": "0.0.11",
  "description": "TS helpers for findy-agent API",
  "main": "dist/lib.js",
  "module": "dist/lib.js",
  "typings": "dist/types/lib.d.ts",
  "bin": "./bin/findy-agent-cli",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc --module commonjs",
    "clean": "rimraf dist",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "licenses:report": "node ./tools/check-licenses.js report ./dist/licenses.txt",
    "licenses:check": "node ./tools/check-licenses.js",
    "preinstall": "npm install --ignore-scripts && node ./dist/tools/download-cli/index.js",
    "update:proto": "./tools/gen-proto.sh",
    "release:patch": "./tools/release.sh patch",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint ./src",
    "test:unit": "jest ./src --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findy-network/findy-common-ts.git"
  },
  "keywords": [
    "ssi",
    "identity",
    "self-sovereign",
    "grpc",
    "client"
  ],
  "author": "findy-network",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/findy-network/findy-common-ts/issues"
  },
  "homepage": "https://github.com/findy-network/findy-common-ts#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "js-green-licenses": "^3.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-protoc-gen": "^0.14.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@terascope/fetch-github-release": "^0.7.7",
    "@types/tar": "^4.0.4",
    "tar": "^6.1.0",
    "winston": "^3.3.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
